---
import MainHeader from "./MainHeader.astro";
import Header from "../components/Header.astro";

const { title, description, head } = Astro.props;
---

<html lang="en">
  <head>
    {head}
    <MainHeader title={title} description={description} />
  </head>
  <body>
    <Header />
    <main tabindex="-1">
      <slot />
    </main>
  </body>
</html>
